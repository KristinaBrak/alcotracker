{
  "configurations": {
    "run": {
      "adapter": "vscode-node",
      "configuration": {
        "request": "launch",
        "protocol": "auto",
        "stopOnEntry": true,
        "console": "integratedTerminal",
        "program": "${workspaceRoot}/src/index.ts",
        "sourceMaps": true,
        "cwd": "${workspaceRoot}",
        "outFiles": [
          "${workspaceRoot}/dist/**/*.js"
        ]
      }
    }
  }
}
